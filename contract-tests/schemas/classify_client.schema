{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "country",
    "request_time"
  ],
  "properties": {
    "country": {
      "$id": "#/properties/country",
      "type": "string",
      "title": "The Country Schema",
      "default": "",
      "examples": [
        "US"
      ],
      "pattern": "^(.*)$"
    },
    "request_time": {
      "$id": "#/properties/request_time",
      "type": "string",
      "title": "The Request_time Schema",
      "default": "",
      "examples": [
        "2019-03-07T19:24:03.741910Z"
      ],
      "pattern": "^(.*)$"
    }
  }
}