{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "approved",
    "approver",
    "comment",
    "created",
    "creator",
    "id"
  ],
  "properties": {
    "approved": {
      "$id": "#/properties/approved",
      "type": "boolean",
      "title": "The Approved Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "approver": {
      "$id": "#/properties/approver",
      "type": "object",
      "title": "The Approver Schema",
      "required": [
        "email",
        "first_name",
        "id",
        "last_name"
      ],
      "properties": {
        "email": {
          "$id": "#/properties/approver/properties/email",
          "type": "string",
          "title": "The Email Schema",
          "default": "",
          "examples": [
            "tdowner@mozilla.com"
          ],
          "pattern": "^(.*)$"
        },
        "first_name": {
          "$id": "#/properties/approver/properties/first_name",
          "type": "string",
          "title": "The First_name Schema",
          "default": "",
          "examples": [
            "Tyler"
          ],
          "pattern": "^(.*)$"
        },
        "id": {
          "$id": "#/properties/approver/properties/id",
          "type": "integer",
          "title": "The Id Schema",
          "default": 0,
          "examples": [
            10
          ]
        },
        "last_name": {
          "$id": "#/properties/approver/properties/last_name",
          "type": "string",
          "title": "The Last_name Schema",
          "default": "",
          "examples": [
            "Downer"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "comment": {
      "$id": "#/properties/comment",
      "type": "string",
      "title": "The Comment Schema",
      "default": "",
      "examples": [
        "+1"
      ],
      "pattern": "^(.*)$"
    },
    "created": {
      "$id": "#/properties/created",
      "type": "string",
      "title": "The Created Schema",
      "default": "",
      "examples": [
        "2017-04-11T22:08:18.526999Z"
      ],
      "pattern": "^(.*)$"
    },
    "creator": {
      "$id": "#/properties/creator",
      "type": "object",
      "title": "The Creator Schema",
      "required": [
        "email",
        "first_name",
        "id",
        "last_name"
      ],
      "properties": {
        "email": {
          "$id": "#/properties/creator/properties/email",
          "type": "string",
          "title": "The Email Schema",
          "default": "",
          "examples": [
            "rrayborn@mozilla.com"
          ],
          "pattern": "^(.*)$"
        },
        "first_name": {
          "$id": "#/properties/creator/properties/first_name",
          "type": "string",
          "title": "The First_name Schema",
          "default": "",
          "examples": [
            "Robert"
          ],
          "pattern": "^(.*)$"
        },
        "id": {
          "$id": "#/properties/creator/properties/id",
          "type": "integer",
          "title": "The Id Schema",
          "default": 0,
          "examples": [
            4
          ]
        },
        "last_name": {
          "$id": "#/properties/creator/properties/last_name",
          "type": "string",
          "title": "The Last_name Schema",
          "default": "",
          "examples": [
            "Rayborn"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        2
      ]
    }
  }
}

